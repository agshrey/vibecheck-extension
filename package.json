{
  "name": "toolbox",
  "displayName": "toolbox",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.100.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "toolbox.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "toolbox.currentTime",
        "title": "Current Time"
      },
      {
        "command": "toolbox.pasteDetector",
        "title": "Paste Detector"
      },
      {
        "command": "toolbox.inlinePasteDetection",
        "title": "VibeCheck: Inline Paste Detection"
      },
      {
        "command": "toolbox.login",
        "title": "VibeCheck: Login"
      },
      {
        "command": "toolbox.logout",
        "title": "VibeCheck: Logout"
      },
      {
        "command": "toolbox.spectatorMode",
        "title": "VibeCheck: Toggle Spectator Mode"
      },
      {
        "command": "toolbox.tabDetection",
        "title": "VibeCheck: Tab Detection"
      }
    ],
    "keybindings": [
      {
        "key": "ctrl+v",
        "command": "toolbox.inlinePasteDetection",
        "when": "editorTextFocus"
      },
      {
        "key": "cmd+v",
        "command": "toolbox.inlinePasteDetection",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/node-fetch": "^2.6.12",
    "@types/vscode": "^1.100.0",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.25.1",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@google/genai": "^1.4.0",
    "@huggingface/inference": "^4.6.1",
    "@supabase/supabase-js": "^2.52.0",
    "@xenova/transformers": "^2.17.2",
    "axios": "^1.11.0",
    "dotenv": "^16.5.0",
    "uuid": "^11.1.0"
  }
}
